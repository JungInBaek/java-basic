package javabasic;

public class Test {

	public static void main(String[] args) {
		// 자동완성 키워드 + ctrl + space
		// 프린트 자동완성 : syso / sysout + ctrl + space
		// 한줄 삭제 : ctrl + d
		// 한줄 복사 : ctrl + alt + 화살표 아래
		// 한줄 이동 : alt + 화살표 방향
		// 저장 : ctrl + s
		// 실행 : ctrl + f11
		// 코드 정리 : ctrl + shift + f
	}
}